const group1 = [0, 1, 0, 0, 1, 1, 0]

function helloBot (people) {

  let result = [];

  for(let i = 0 ; i < people.length ; i++) {
    if(people[i] === 0) {
      result.push('안녕하세요')
    }
    else if(people[i] === 1) {
      result.push('또 만나네요')
    }
  }

  return result; 
  
}

console.log(helloBot(group1));
